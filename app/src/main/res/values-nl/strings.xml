    <string name="setting_summary_remote">Enable remote content, which is not directly from the website you are visiting. Disabling this is more secure, faster and saves data volume. But some content will not be displayed anymore.</string>
    <string name="setting_title_save_data">Verzoek \'Gegevens opslaan\'</string>
